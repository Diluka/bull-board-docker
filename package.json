{
  "name": "bull-board-docker",
  "version": "5.3.0",
  "main": "src/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node .",
    "pretest": "npm run test:stack:up",
    "test": "docker compose -f docker-compose.test.yml run --rm sut",
    "posttest": "npm run test:stack:down",
    "test:stack:up": "docker compose -f docker-compose.test.yml up -d --build bull-board",
    "test:stack:down": "docker compose -f docker-compose.test.yml down --rmi local",
    "prepare": "husky install"
  },
  "repository": "https://github.com/Diluka/bull-board-docker",
  "dependencies": {
    "@bull-board/api": "^5.18.1",
    "@bull-board/express": "^5.18.1",
    "bull": "^4.12.8",
    "bullmq": "^5.7.10",
    "connect-ensure-login": "^0.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "ioredis": "^5.4.1",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.4",
    "prettier": "^3.2.5"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 140,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2021
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
