{
  "name": "bull-board-docker",
  "version": "6.5.1",
  "type": "module",
  "main": "src/index.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "deno run -A src/index.ts",
    "dev": "deno run -A --watch src/index.ts",
    "pretest": "npm run test:stack:up",
    "test": "docker compose -f docker-compose.test.yml run --rm sut",
    "posttest": "npm run test:stack:down",
    "test:stack:up": "docker compose -f docker-compose.test.yml up -d --build bull-board",
    "test:stack:down": "docker compose -f docker-compose.test.yml down --rmi local"
  },
  "repository": "https://github.com/Diluka/bull-board-docker",
  "dependencies": {
    "@bull-board/api": "^6.12.0",
    "@bull-board/express": "^6.12.0",
    "bull": "^4.16.5",
    "bullmq": "^5.58.0",
    "connect-ensure-login": "^0.1.1",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "ioredis": "^5.7.0",
    "morgan": "^1.10.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@types/connect-ensure-login": "^0.1.9",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.3.0",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38"
  }
}
