{
  "name": "bull-board-docker",
  "version": "6.1.0",
  "type": "module",
  "main": "src/index.mjs",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node .",
    "pretest": "npm run test:stack:up",
    "test": "docker compose -f docker-compose.test.yml run --rm sut",
    "posttest": "npm run test:stack:down",
    "test:stack:up": "docker compose -f docker-compose.test.yml up -d --build bull-board",
    "test:stack:down": "docker compose -f docker-compose.test.yml down --rmi local",
    "prepare": "husky"
  },
  "repository": "https://github.com/Diluka/bull-board-docker",
  "dependencies": {
    "@bull-board/api": "^6.7.9",
    "@bull-board/express": "^6.7.9",
    "bull": "^4.16.5",
    "bullmq": "^5.41.7",
    "connect-ensure-login": "^0.1.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "ioredis": "^5.5.0",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.2"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 140,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2023
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
